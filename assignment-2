program on dictionary

def check(a):
    # Create empty dict
    gmail = dict()
    # Fill it with some values
    gmail['aaa@gmail.com'] = '1'
    gmail['bbb@gmail.com'] = '2'
    gmail['ccc@gmail.com'] = '3'
    gmail['xxx@gmail.com'] = '5'

    ids = ['1','4','3','2']

    for x in gmail:
        if a in gmail[x]:
            print('The ' + x + ' is matched')
        else:
            print('The ' + x + ' is not matched')

a=input("Enter id:")
check(a)
